# Mysql server info
mysql_host = "localhost"
mysql_user = "dialeruser"
mysql_pass = "dialermysqlpwd"
mysql_dbname = "nipodialer"

# Logging settings
log_to_file = True
log_file_path = '/var/log/nipodialer'
sounds_file_path = 'custom'

# Debug settings
force_number = False